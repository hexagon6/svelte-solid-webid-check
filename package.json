{
  "name": "@hexagon6/svelte-solid-webid-check",
  "version": "0.3.0",
  "scripts": {
    "story:dev": "histoire dev",
    "story:build": "histoire build",
    "story:preview": "histoire preview",
    "dev": "vite dev",
    "build": "svelte-kit sync && svelte-package",
    "prepublishOnly": "echo 'Did you mean to publish `./dist/`, instead of `./`?' && exit 1",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "devDependencies": {
    "@histoire/plugin-svelte": "^0.17.9",
    "@histoire/shared": "^0.17.9",
    "@sveltejs/adapter-auto": "^3.1.1",
    "@sveltejs/kit": "^2.5.0",
    "@sveltejs/package": "^2.2.6",
    "histoire": "^0.17.9",
    "prettier": "^3.2.4",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte-check": "^3.0.1",
    "tslib": "^2.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.12"
  },
  "node": ">=16",
  "exports": {
    ".": "./index.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hexagon6/svelte-solid-webid-check.git"
  },
  "type": "module",
  "peerDependencies": {
    "svelte": "^4.2.9"
  }
}
